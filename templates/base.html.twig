<!DOCTYPE html>
{% block htmlheader %}
<html lang="en">
{% endblock %}
<head>
    <meta charset="utf-8">
    <title>{% block title %}FeedBox{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Selfhosted rss reader">
    <meta name="author" content="Danilo KÃ¼hn">
    <meta name="apple-mobile-web-app-title" content="Feedbox">
    <link href="{{ base_url }}/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ base_url }}/css/font-awesome.min.css" rel="stylesheet">
    <link href="{{ base_url }}/css/style.css" rel="stylesheet">
    {% block stylesheets %}
    {{ stylesheets|raw }}
    {% endblock %}
    <link rel="shortcut icon" href="{{ base_url }}/img/feedbox.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="/img/apple-touch-icon-precomposed.png" />
</head>
<body>
{% block content %}
{{ content|raw }}
{% endblock %}

{% block javascript %}
{{ javascript|raw }}
{% endblock %}
</body>
</html>
